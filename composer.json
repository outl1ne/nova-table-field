{
  "name": "optimistdigital/nova-table-field",
  "description": "Table field for Laravel Nova",
  "keywords": [
    "laravel",
    "nova",
    "tables",
    "optimistdigital"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Kaspar Rosin",
      "email": "kaspar.rosin@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Optimist Digital",
      "email": "info@optimistdigital.com",
      "role": "Maintainer"
    },
    {
      "name": "Tarvo Reinpalu",
      "email": "tarvoreinpalu@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "laravel/nova": "^1.0 || ^2.0 || ^3.0",
    "optimistdigital/nova-tailwind": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Optimistdigital\\NovaTableField\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Optimistdigital\\NovaTableField\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
